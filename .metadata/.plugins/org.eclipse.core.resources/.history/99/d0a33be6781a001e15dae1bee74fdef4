package com.saleofdrink.model;


import jakarta.persistence.Column;
import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;

import jakarta.persistence.Table;

@Entity
@Table (name = "venta")
public class Venta {
	
	@Id
	@GeneratedValue(strategy = GenerationType.AUTO)
	@Column(name = "IDVEN")
	private Long idven;
	
	@Column(name = "FECVEN")
	private String fecven;
	
	
	private String TIPPAGVEN;
	
	
	private String COMVEN;
		
	public Venta() {
		
	}
	
	public Venta(String fECVEN, String tIPPAGVEN, String cOMVEN) {
		super();
		FECVEN = fECVEN;
		TIPPAGVEN = tIPPAGVEN;
		COMVEN = cOMVEN;
	}

	public Long getIDVEN() {
		return IDVEN;
	}

	public void setIDVEN(Long iDVEN) {
		IDVEN = iDVEN;
	}

	public String getFECVEN() {
		return FECVEN;
	}

	public void setFECVEN(String fECVEN) {
		FECVEN = fECVEN;
	}

	public String getTIPPAGVEN() {
		return TIPPAGVEN;
	}

	public void setTIPPAGVEN(String tIPPAGVEN) {
		TIPPAGVEN = tIPPAGVEN;
	}

	public String getCOMVEN() {
		return COMVEN;
	}

	public void setCOMVEN(String cOMVEN) {
		COMVEN = cOMVEN;
	}

	
}
